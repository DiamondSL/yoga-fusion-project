{
  "kind": "collectionType",
  "collectionName": "classes",
  "info": {
    "singularName": "class",
    "pluralName": "classes",
    "displayName": "Class",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "Discipline": {
      "type": "component",
      "repeatable": false,
      "component": "shared.method"
    },
    "Intensity": {
      "type": "enumeration",
      "enum": [
        "low - 1",
        "medium - 2",
        "high - 3"
      ],
      "default": "low - 1"
    },
    "Information": {
      "type": "string",
      "minLength": 1
    },
    "Photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Description": {
      "type": "blocks"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "class"
    },
    "teachers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::teacher.teacher",
      "inversedBy": "classes"
    },
    "Time": {
      "type": "component",
      "repeatable": false,
      "component": "shared.event-time"
    }
  }
}
